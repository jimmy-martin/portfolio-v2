{% extends "back/base.html.twig" %}

{% block title %}
	Modifier une catégorie
	-
	{{ parent() }}
{% endblock %}

{% block body %}
	<div class="col-12">
		<h1 class="mb-3">Modifier une catégorie</h1>
	</div>
	{{ form_start(form) }}

    {{ form_row(form.name) }}
    {{ form_row(form.projects) }}
    
    <button class="btn btn-primary" type="submit">Modifier</button>
	{{ form_end(form) }}
{% endblock %}
